#include <stdio.h>
#include <stdlib.h>

int main(){
    float h;
    float l;
    float ml;
    int latastotais;

    printf("Altura em metros: ");
    scanf("%f",&h);
    printf("Largura em metros: ");
    scanf("%f",&l);
    ml=(l*h)*300;
    scanf("f",&ml);
    latastotais= (int)ml/2000;
    if(ml/2000>latastotais){
        printf("%i latas de tinta",latastotais+1);
    }


return 0;

}
